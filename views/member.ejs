              <script>
                function verify(memberId) {
                  alert("verify function called");
                
                  // Get the selected radio button value
                  const status = document.querySelector(`input[name="status_${memberId}"]:checked`).value;
                
                  // Get the reason from the text field
                  const reason = document.getElementById(`reason_${memberId}`).value;
                  const role = document.getElementById(`role_${memberId}`).value;
                
                  // Make a PUT API request to update member data
                  const apiUrl = `http://localhost:8000/memberdataupdate`;
                
                  fetch(apiUrl, {
                    method: 'PUT',
                    headers: {
                      'Content-Type': 'application/json',
                    },
                    body: JSON.stringify({ status, reason, memberId,role}),
                  })
                  .then(response => {
                    if (!response.ok) {
                      throw new Error(`API request failed with status ${response.status}`);
                    }
                    return response.json();
                  })                             
                  .then(data => {
                    console.log('Member data updated successfully:', data);
                    // Handle success (e.g., show a success message)
                  })
                  .catch(error => {
                    console.error('Error updating member data:', error);
                    // Handle error (e.g., show an error message)
                  });
                }
                </script>
              
              <script>
                function sendVerificationEmail(memberId) {
                    alert("sendVerificationEmail function called");
                    const status = document.querySelector(`input[name="status_${memberId}"]:checked`).value;
                
                // Get the reason from the text field
                const reason = document.getElementById(`reason_${memberId}`).value;
                const role = document.getElementById(`role_${memberId}`).value;
                    // Get the email value from the input field
                    const email = document.getElementById(`email_${memberId}`).value;
            
                    // Make a POST API request to update member data
                    const apiUrl = `http://localhost:8000/mail2`;
            
                    fetch(apiUrl, {
                        method: 'post', // Correct method name
                        headers: {
                            'Content-Type': 'application/json',
                        },
                        body: JSON.stringify({ memberId, email,status,role,reason }),
                    })
                    .then(response => {
                        if (!response.ok) {
                            throw new Error(`API request failed with status ${response.status}: ${response.statusText}`);
                        }
                        return response.json();
                    })
                    .then(data => {
                        console.log('Email sent successfully:', data);
                        // Handle success (e.g., show a success message)
                    })
                    .catch(error => {
                        console.error('Error occurred during email sending:', error);
                        // Handle error (e.g., show an error message)
                    });
                }
            </script>
            
              
                  
